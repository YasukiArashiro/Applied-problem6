<div class="container col-xs-4 col-xs-offset-5" style="margin-top: 70px;">
	<%= form_tag(search_search_path, method: :get) do %>
		<%= text_field_tag :search %>
		<%= select_tag 'range', options_for_select({User: 'user', Book: 'book'}) %>
		<%= select_tag 'match', options_for_select({完全一致: 'perfect', 前方一致: 'start', 後方一致:'back', 部分一致:'part'}) %>
		<%= submit_tag 'Search', name: nil %>
	<% end %>
</div>